{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "globals": {
      "player": "readonly",
      "client": "readonly"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    },
    "ignorePatterns": ["node_modules/", "package-lock.json"]
  }
  